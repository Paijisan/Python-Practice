class Bank:
    def __init__(self):
        self.accounts = []

    def add_account(self, account):
        self.accounts.append(account)

    def find_account(self, owner_name):
        for acc in self.accounts:
            if acc.owner == owner_name:
                return acc
        return None

    def __str__(self):
        if not self.accounts:
            return "Bank has no accounts."

        return "\n".join(str(acc) for acc in self.accounts)


# Example usage
b = Bank()

acc1 = BankAccount("Paige", 500)
acc2 = BankAccount("Alex", 200)

b.add_account(acc1)
b.add_account(acc2)

print(b)

found = b.find_account("Paige")
if found:
    print("Found:", found)
